<div class="container">
  <h2 class="h2Search">All your matching skills </h2>

      <table class="list-skills">
  {{#each skills }}
<tr class="tr">
    <td class="tdPicture "><dev class="imgSkills"><img src="{{picture}}" alt=""></dev></td>
    <td><p>{{ skillName }}</p></td>
    <td><a class="btn-skills" href="/details/{{_id}}">Info</a></td>
    <br>
  </tr>
  {{/each}}
 
</table>

    {{#if message}}
    <div class="errorMsg">
    <p>{{message}}</p>
    </div>
    <img
      src="data:image/jpeg;base64,"
      alt="">
    {{/if}}

<div class="container">
  <a class="big-btn containerReturn" href="/skills">Return</a>
</div>
</div>